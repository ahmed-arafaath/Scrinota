{
    "manifest_version": 3,
    "name": "Scrinota",
    "description": "This extension allows you to set reminder and save notes",
    "version": "1.0",
    "background": {"service_worker": "background.js"},
    "icons": {
      "128": "images/logo128.png",
      "48": "images/logo48.png",
      "16": "images/logo16.png"
    },
    "action": {
      "default_icon": {
        "128": "images/logo128.png",
        "48": "images/logo48.png",
        "16": "images/logo16.png"
      },
      "default_popup": "popup.html",
      "default_title": "Scrinota"
    },
    "permissions": [
            "storage",
            "notifications",
            "tabs",
            "alarms"
          ]
  }